{
  "description": "__MSG_extensionDescripcion__",
  "manifest_version": 2,
  "name": "__MSG_extensionNombre__",
  "default_locale": "en",
  "version": "10.0",
  "icons": {
    "480": "icons/youtube.png"
  },
  "applications": {
    "gecko": {
      "id": "youtube.start.at@elinv.com",
      "strict_min_version": "57.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/watch?v=*"
      ],
      "run_at": "document_start",
      "js": [
        "./scr/funciones.js",
        "./scr/content-script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "./scr/funciones.js",
      "./scr/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/youtube.png",
    "default_title": "__MSG_extensionTitle__",
    "default_popup": "./popup/youtubeStartAtInfo.html"
  }
}